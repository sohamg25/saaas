        <script type="text/javascript" language="javascript">

            $(function () {

                $('#discussion_search_button').click(search);

                $('#discussion_search').keydown(function (e) {
                    if ($keyCode(e) == 13) return search();
                });

                function search() {
                    CodePlex.UpdateProgress.displayPanelAndDisableControls('Loading...');
                    window.location = 'http://z3.codeplex.com/discussions/topics/general' + '?searchText=' + $('#discussion_search').val();
                    return false;
                }
            });

            function getSearchParams() {
                var params = "?searchText=" + $('#discussion_search').val() + '&page=0&size=10';
                return params;
            }

            CodePlexPaging.initialize('getSearchParams');

        </script>

