    <script language="javascript">
        $(document).ready(function () {            
            
            $('#SelectedBranch').change(function (e) {

                var branch = $(this).val(),
                    url = 'http://z3.codeplex.com/SourceControl/latest';
                
                if (branch.length > 0)
                    url += '?branch=' + branch;

                location.href = url;
            });
            

            // Open the inital file if present
            var initalFile = 'README';
            if (initalFile != null && initalFile.length > 0 && location.hash.length == 0) {
                location.hash = "#" + initalFile;
            }

            $(document).sourceCodeBrowser({
                fileUrl: 'http://z3.codeplex.com/SourceControl/changeset/file/view/89c1785b73225a1b363c0e485f854613121b70a7',
                listUrl: 'http://z3.codeplex.com/SourceControl/changeset/list/89c1785b73225a1b363c0e485f854613121b70a7?path=xXPathXx&parentKey=xXKeyXx',
                diffUrl: 'http://z3.codeplex.com/SourceControl/diff/file/view/89c1785b73225a1b363c0e485f854613121b70a7?fileId=-1',
                oldDiffUrl: 'http://z3.codeplex.com/SourceControl/diff/file/view/89c1785b73225a1b363c0e485f854613121b70a7?fileId=-1&olderChangeSetId=-2',
                changesetsUrl: 'http://z3.codeplex.com/sourcecontrol/file/changesets?itemId=-1&currentChangesetId=89c1785b73225a1b363c0e485f854613121b70a7',
                contactUsUrl: 'https://www.codeplex.com/site/contact'
            });
        });

    </script>
